{
  "name": "tds-integration-mock-services",
  "version": "1.0.0",
  "description": "Mock services for TDS Integration Platform local development",
  "main": "mock-tds-server.js",
  "scripts": {
    "start": "node mock-tds-server.js",
    "start:tds": "node mock-tds-server.js --port 3001",
    "start:alto": "node mock-alto-server.js --port 3002",
    "start:all": "concurrently \"npm run start:tds\" \"npm run start:alto\"",
    "test": "jest",
    "dev": "nodemon mock-tds-server.js",
    "clean": "rimraf node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "rimraf": "^5.0.5"
  },
  "overrides": {
    "rimraf": "^5.0.5",
    "glob": "^10.3.10",
    "uuid": "^9.0.1",
    "inflight": "npm:@isaacs/inflight@^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "mock",
    "tds",
    "alto",
    "testing",
    "development"
  ],
  "author": "Omar Lodhi",
  "license": "MIT"
}